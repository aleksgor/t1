<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<serverModel xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../serverModel.xsd">
    <storeModels>
        <storeModel>
            <dataSource>a</dataSource>
            <model>MainTestModel</model>
            <class>com.ag.cache.test.model.MainTestModel</class>
            <readThrough>true</readThrough>
            <writeThrough>true</writeThrough>
            <copyCount>2</copyCount>
            <serverType>CACHE</serverType>

            <storeType>BYTES</storeType>
        </storeModel>
    </storeModels>
    <listeners>
        <listener>
            <host>localhost</host>
            <port>2322</port>
            <backlog>10</backlog>
            <minThreads>5</minThreads>
            <maxThreads>5</maxThreads>
            <status>1</status>
            <properties />
        </listener>
    </listeners>
    <commandPlugins>
        <commandPlugin>
            <checkDelay>3000</checkDelay>
            <class>com.ag.plugin.IdGenerator</class>
            <poolSize>10</poolSize>
            <timeout>10000</timeout>
        </commandPlugin>
    </commandPlugins>
    <servers />
    <clients>
        <connect>
            <status>Unknown</status>
            <threads>8</threads>
            <managementClient> <!-- <managementClient> -->
                <host>localhost</host>
                <port>2142</port>
                <threads>2</threads>
                <timeout>1000</timeout>
            </managementClient>

            <listener>
                <host>localhost</host>
                <port>2322</port>
                <backlog>1000</backlog>
                <minThreads>8</minThreads>
                <maxThreads>8</maxThreads>
                <status>0</status>
                <properties />
            </listener>
        </connect>
    </clients>
    <dataSources>
        <dataSource>
            <name>a</name>
            <class>com.ag.cache.test.userdataaccess.BaseDataInvoker</class>
            <threads>10</threads>
            <timeout>10000</timeout>
            <properties>
                <entry>
                    <key>threads</key>
                    <value>10</value>
                </entry>
                <entry>
                    <key>driver</key>
                    <value>org.hsqldb.jdbcDriver</value>
                </entry>
                <entry>
                    <key>password</key>
                    <value></value>
                </entry>
                <entry>
                    <key>user</key>
                    <value>sa</value>
                </entry>
                <entry>
                    <key>url</key>
                    <value>jdbc:hsqldb:hsql://localhost/test</value>
                </entry>
            </properties>
        </dataSource>
    </dataSources>
    <sessionClients />
    <calculateStatistic>true</calculateStatistic>
    <saveClients>
        <saveClient>
            <host>localhost</host>
            <port>5824</port>
            <threads>2</threads>
            <timeout>1000</timeout>
            <protocolType>UDP</protocolType>
        </saveClient>
    </saveClients>
    <serverName>cache2</serverName>
    <managementServer>
        <host>localhost</host>
        <port>2342</port>
        <minThreads>10</minThreads>
        <maxThreads>20</maxThreads>
        <keepAliveTime>1000</keepAliveTime>
    </managementServer>
    <trustSessions>true</trustSessions>
</serverModel>
