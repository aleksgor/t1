<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<serverModel xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../serverModel.xsd">
    <storeModels>
        <storeModel>
            <dataSource>a</dataSource>
            <model>Child</model>
            <class>com.ag.cache.test.model.Child</class>
            <readThrough>true</readThrough>
            <writeThrough>true</writeThrough>
            <copyCount>2</copyCount>
            <serverType>CACHE_MANAGER</serverType>

            <storeType>BYTES</storeType>
        </storeModel>
        <storeModel>
            <dataSource>a</dataSource>
            <model>MainTestModel</model>
            <class>com.ag.cache.test.model.MainTestModel</class>
            <readThrough>true</readThrough>
            <writeThrough>true</writeThrough>
            <copyCount>2</copyCount>
            <serverType>CACHE_MANAGER</serverType>

            <storeType>BYTES</storeType>
        </storeModel>
    </storeModels>
    <listeners>
        <listener>
            <host>localhost</host>
            <port>2122</port>
            <backlog>10</backlog>
            <minThreads>20</minThreads>
            <maxThreads>25</maxThreads>
            <status>1</status>
            <protocolType>SSL</protocolType>
            <properties>
                <entry>
                    <key>javax.net.ssl.keyStore</key>
                    <value>src/test/resources/keystore.jks</value>
                </entry>
                <entry>
                    <key>javax.net.ssl.keyStorePassword</key>
                    <value>storePassword</value>
                </entry>
                <entry>
                    <key>javax.net.ssl.keyPassword</key>
                    <value>keypassword</value>
                </entry>

            </properties>
        </listener>
    </listeners>
    <commandPlugins>
        <commandPlugin>
            <checkDelay>3000</checkDelay>
            <class>com.ag.plugin.IdGenerator</class>
            <poolSize>10</poolSize>
            <timeout>10000</timeout>
        </commandPlugin>
    </commandPlugins>
    <servers />
    <clients />
    <dataSources>
    </dataSources>
    <sessionClients>
        <sessionClient>
            <host>localhost</host>
            <port>5888</port>
            <threads>5</threads>
            <timeout>1000</timeout>
        </sessionClient>
    </sessionClients>
    <calculateStatistic>true</calculateStatistic>
    <saveClients>
        <saveClient>
            <host>localhost</host>
            <port>5824</port>
            <threads>2</threads>
            <timeout>1000</timeout>
        </saveClient>
    </saveClients>
    <saveServers>
        <saveServer>
            <host>localhost</host>
            <port>5824</port>
            <minThreads>16</minThreads>
            <maxThreads>21</maxThreads>
            <keepAliveTime>3000</keepAliveTime>
            <sessionTimeout>10000</sessionTimeout>
        </saveServer>
    </saveServers>
    <serverName>cacheManager</serverName>
    <sessionServer>
        <host>localhost</host>
        <port>5888</port>
        <minThreads>25</minThreads>
        <maxThreads>25</maxThreads>
        <keepAliveTime>3000</keepAliveTime>
        <sessionTimeLive>18000000</sessionTimeLive>
        <authorizationService>
            <clazz>com.ag.authorization.FileAuthorizationServiceImpl</clazz>
            <properties>
                <entry>
                    <key>file</key>
                    <value>users.txt</value>
                </entry>
            </properties>
        </authorizationService>
    </sessionServer>
    <managementServer>
        <host>localhost</host>
        <port>2142</port>
        <minThreads>28</minThreads>
        <maxThreads>28</maxThreads>
        <keepAliveTime>3000</keepAliveTime>
    </managementServer>
    <trustSessions>false</trustSessions>
</serverModel>
