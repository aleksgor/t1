    <saveClients>
        <saveClient>
            <host>localhost</host>
            <port>5828</port>
            <threads>5</threads>
            <timeout>1000</timeout>
            <protocolType>UDP</protocolType>
        </saveClient>
    </saveClients>
    <serverName>cache4</serverName>
    <managementServer>
        <host>localhost</host>
        <port>2442</port>
        <minThreads>10</minThreads>
        <maxThreads>20</maxThreads>
