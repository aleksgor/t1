        </commandPlugin>
    </commandPlugins>
    <servers />
    <clients>
        <connect>
            <status>Unknown</status>
            <threads>4</threads>
            <managementClient>
                <host>localhost</host>
                <port>2042</port>
                <threads>8</threads>
                <timeout>1000</timeout>
            </managementClient>
            <listener>
                <host>localhost</host>
                <port>2422</port>
                <backlog>1000</backlog>
                <minThreads>4</minThreads>
                <maxThreads>4</maxThreads>
                <status>0</status>
                <properties />
            </listener>
        </connect>
    </clients>
    <dataSources>
        <dataSource>
            <name>msql</name>
            <class>com.ag.cache.test.userdataaccess.BaseDataInvoker</class>
            <threads>10</threads>
            <timeout>10000</timeout>
            <properties>
                <entry>
                    <key>fileConfiguration</key>
                    <value>pm2.cfg.xml</value>
                </entry>
                <entry>
                    <key>threads</key>
                    <value>10</value>
                </entry>
                <entry>
                    <key>driver</key>
                    <value>org.hsqldb.jdbcDriver</value>
                </entry>
                <entry>
                    <key>password</key>
                    <value></value>
                </entry>
                <entry>
                    <key>user</key>
                    <value>sa</value>
                </entry>
                <entry>
                    <key>url</key>
                    <value>jdbc:hsqldb:hsql://localhost/test</value>
                </entry>
            </properties>
        </dataSource>
    </dataSources>
    <sessionClients />
