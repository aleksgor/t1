<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<serverModel xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="../serverModel.xsd">
    <storeModels>
           <storeModel>
            <dataSource>msql</dataSource>
            <model>Price</model>
            <class>com.ag.cache.test.model.Price</class>
            <readThrough>true</readThrough>
            <writeThrough>true</writeThrough>
            <copyCount>2</copyCount>
            <serverType>CACHE</serverType>
            <storeType>BYTES</storeType>
        </storeModel>
        <storeModel>
            <dataSource>msql</dataSource>
            <model>MainTestModel</model>
            <class>com.ag.cache.test.model.MainTestModel</class>
            <readThrough>true</readThrough>
            <writeThrough>true</writeThrough>
            <copyCount>1</copyCount>
            <serverType>CACHE</serverType>
            <storeType>BYTES</storeType>
            <cacheMatcher>
                <class>com.ag.cache.test.cacheMatcher.EvennessMatcher</class>
                <properties>
                    <entry>
                        <key>even</key>
                        <value>true</value>
                    </entry>
                </properties>
            </cacheMatcher>
        </storeModel>
        <storeModel>
            <dataSource>msql</dataSource>
            <model>Price</model>
            <class>com.ag.cache.test.model.Price</class>
            <readThrough>true</readThrough>
            <writeThrough>true</writeThrough>
            <copyCount>2</copyCount>
            <serverType>CACHE</serverType>
            <storeType>BYTES</storeType>
        </storeModel>        
    </storeModels>
    <listeners>
        <listener>
            <host>localhost</host>
            <port>2422</port>
            <backlog>10</backlog>
            <minThreads>5</minThreads>
            <maxThreads>5</maxThreads>
            <status>1</status>
            <properties />
        </listener>
    </listeners>
    <commandPlugins>
        <commandPlugin>
            <checkDelay>3000</checkDelay>
            <class>com.ag.plugin.IdGenerator</class>
            <poolSize>10</poolSize>
